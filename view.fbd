# vim:fileencoding=utf-8:fdm=marker:ft=conf

read foo.frd
read foo.inp

capt Title
ulin subtitle

# Mirror in x-axis
#seta base all
#copy base new mir x
#comp new do

# Display total displacement
ds 1 e 4
